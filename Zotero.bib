@article{ghiani_new_2007,
	title = {New policies for the dynamic traveling salesman problem},
	volume = {22},
	issn = {1055-6788},
	url = {http://dx.doi.org/10.1080/10556780701550026},
	doi = {10.1080/10556780701550026},
	abstract = {The \textit{dynamic traveling salesman problem} is a variant of the classical \textit{traveling salesman problem} in which service requests arrive in a random fashion. In this paper we propose three new routing policies minimizing the expected total waiting time of all the demands in the system. In particular, we show that two policies are asymptotically optimal in heavy traffic, whereas the third one has a constant factor guarantee of two. We also perform extensive simulations in order to compare the three policies under various arrival rates.},
