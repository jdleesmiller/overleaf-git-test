	author = {Kelly, F. P. and Maulloo, A. K. and Tan, D. K. H.},
	year = {1998},
	pages = {237--252}
}

@article{hackner_deregulating_1995,
	title = {Deregulating Taxi Services: A Word of Caution},
	volume = {29},
	issn = {00225258},
	url = {http://dx.doi.org/10.2307/20053073},
	doi = {10.2307/20053073},
	abstract = {This paper studies pricing and capacity decisions in markets for phone-ordered taxicabs. Taxi firms first choose capacities and then compete in prices. As firm demand increases, so does waiting time. This dampens competition and makes prices too high from the social point of view. Efficiency improves if firms choose large capacities. In a two-firm setting, equilibrium capacities are shown to be larger if both firms maximise total profits than if they maximise profits per cab. Hence, if fixed costs for entrant cabs are small, the market is more efficient in the former case. /// Dieser Aufsatz analysiert Preis- und Kapazitätsentscheidungen im Markt der telefonisch bestellten Taxis. Taxiunternehmen fällen erst die Kapazitäts- und erst dann die Preisentscheidung. Bei Nachfragewachstum erhöht sich zunächst die Wartezeit und die Preise steigen auf ein Niveau jenseits des sozialen Optimums. Erst wenn die Unternehmen grö{\textbackslash}s sere Kapazitäten schaffen, steigt die Effizienz. In einem Modell mit zwei Taxibetrieben liegt die Gleichgewichtskapazität höher, wenn beide Betriebe den Unternehmensgewinn maximieren als bei Maximierung des Gewinns pro Taxi. Folglich ist im ersteren Fall der Markt effizienter, solange die Fixkosten für neu in den Markt eintretende Taxis gering sind. /// Cet article étudie les décisions de tarification et de capacité dans des marchés pour taxis appelés par téléphone. Les sociétés de taxi choisissent d'abord les capacités, puis elles se font concurrence sur les prix. Quand la demande de société augmente, il en est de même pour le temps d'attente. Cela étouffe la compétition et fait que les prix deviennent trop élevés du point de vue social. L'efficacité est améliorée si les sociétés choisissent de larges capacités. Dans une situation où deux sociétés sont présentes, il est montré que les capacités d'équilibre sont plus grandes dans le cas où les deux sociétés maximisent les bénéfices totaux plutôt que si elles maximisent les bénéfices par voiture de taxi. Donc, si les coûts fixes pour les taxis entrant sur le marché sont faibles, le marché est plus efficace dans le cas précédent. /// Este artículo analiza la decisiones de precio y capacidad en los mercados de radio-taxis. Las empresas de taxi primero eligen capacidades y luego compiten en precios. Cuando la demanda de la empresa aumenta, también lo hace el tiempo de espera. Esto reduce la competencia y hace que los precios sean demasiado altos desde el punto de vista social. La eficiencia mejora si las empresas fijan capacidades altas. En un escenario de dos empresas, se demuestra que las capacidades de equilibrio son mayores cuando ambas empresas maximizan los beneficios totales que cuando ambas maximizan los beneficios por vehículo. Por lo tanto, si los costes fijos so pequeños para los taxis entrantes, el mercado es más eficiente en el primer caso.},
	number = {2},
	journal = {Journal of Transport Economics and Policy},
	author = {Häckner, Jonas and Nyberg, Sten},
	year = {1995}
}

@incollection{gendreau_dynamic_1998,
	address = {Boston},
	title = {Dynamic Vehicle Routing and Dispatching},
	isbn = {0792381610},
	url = {http://citeseerx.ist.psu.edu/viewdoc/summary?doi=10.1.1.41.2841},
	abstract = {Dynamic vehicle routing and dispatching refers to a wide range of problems where information on the problem is revealed to the decision maker concurrently with the determination of the solution. These problems have recently emerged as an active area of research due to recent technological advances that allow real-time information to be quickly obtained and processed. There are probably as many variants of these problems as there are real-world applications. After a brief overview of this broad domain, the paper will then focus on problems motivated by courier services and demand responsive transportation systems. These systems typically evolve within a local service area over a relatively short period of time (typically, a few hours), thus putting stringent response time requirements on the decision maker. They are also characterized by a strong routing component: many tasks can be allocated at once to the same vehicle and these tasks must be appropriately sequenced. The ...},
