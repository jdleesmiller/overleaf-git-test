    posted-at = {2009-05-20 17:43:51},
    priority = {2},
    title = {Modulation of septo-hippocampal [theta] activity by {GABAA} receptors: an experimental and computational approach},
    url = {http://dx.doi.org/10.1016/j.neuroscience.2004.03.043},
