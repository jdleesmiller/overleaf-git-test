@article{Parker1998Personal,
    abstract = {Since late 1993, The Regional Transportation Authority and its consultant, Consoer Townsend Envirodyne Engineers, Inc. have been involved in a development program to produce the first-ever operational Personal Rapid Transit ({PRT}) System. As this development process has evolved, it has become increasingly evident that a marketable system will soon be available for deployment in many locales throughout the world. Although many of us have heard and read about {PRT} systems, no one has ever built one for revenue or non-revenue service. Consequently, a considerable amount of speculation exists as to the viability of such a system in todays transit world. {PRT} systems must have a distinct advantage over other forms of transportation in order to make it attractive and be able to be implemented in an environment which is primarily hostile to anything which violates the suburban landscape. In order to achieve these objectives, a {PRT} system must be flexible, fast, and most importantly, economical to operate. In addition, a {PRT} system must be able to achieve integration into a community in the least obtrusive manner possible and, at the same time, serve its ridership base conveniently. This abstract will look at one form of use for a {PRT} system. That use will be to serve as an intermodal link between one alternative form of transportation in the suburbs. This paper will address the following: Alignment Stations Maintenance Yard Ridership Fare Management The {PRT} system depicted is a fictional alignment which could be located in any number of suburban locales acid does not reflect any long range planning concept. Stations will also be discussed; as to how these stations will be handled to fit into the community yet situated to handle the anticipated ridership as well. The paper will address how the needs of the system are to be met and the facilities required to serve this system. Ultimately, the success of {PRT} will be judged by its ability to attract sustainable ridership as well as recover {O\&M} expenditures through the farebox. To that end, this paper will examine ridership potential and fare management strategies afforded by this technology.},
    author = {Parker, T. J. and Mizera, R. F.},
    citeulike-article-id = {4562984},
    journal = {Automated People Movers Vi},
    keywords = {file-import-09-05-20},
    posted-at = {2009-05-20 17:43:38},
    priority = {2},
    title = {Personal Rapid Transit ({PRT}) - An intermodal connector},
    year = {1998}
}

@article{Pepyne1997Optimal,
    abstract = {In this paper we develop optimal dispatching controllers for elevator systems during uppeak traffic, An uppeak traffic period arises when the bulk of the passenger traffic is moving from the first floor up into the building (e.g., the start of a business day in an office building). The cars deliver the passengers and then return empty to the first door to pick up more passengers. We show that the structure of the optimal dispatching policy minimizing the discounted or average passenger waiting time is a threshold-based policy, That is, the optimal policy is to dispatch an available car from the first floor when the number of passengers inside the car reaches or exceeds a threshold that depends on several factors including the passenger arrival rate, elevator performance capabilities, and the number of elevators available at the first door. Since most elevator systems have sensors to determine the car locations and the number of passengers in each car, such a threshold policy is easily implemented, Our analysis is based on a Markov decision problem formulation with a batch service queueing model consisting of a single queue served by multiple finite-capacity bulk servers, We use dynamic programming techniques to obtain the structure of the optimal control policy and to derive some of its important properties, Several numerical examples are included to illustrate our results and to compare the optimal threshold policy to some known ad hoc approaches, Finally, since many transportation systems can be modeled as multiserver batch service queueing systems, we expect our results to be useful in controlling those systems as well.},
    author = {Pepyne, D. L. and Cassandras, C. G.},
    citeulike-article-id = {4562983},
    journal = {Ieee Transactions on Control Systems Technology},
    keywords = {file-import-09-05-20},
    number = {6},
    posted-at = {2009-05-20 17:43:38},
    priority = {2},
    title = {Optimal dispatching control for elevator systems during uppeak traffic},
    volume = {5},
    year = {1997}
}

@article{Pepyne1998Design,
    abstract = {We design a dispatching controller for elevator systems during uppeak passenger traffic with the ability to adapt to changing operating conditions. The design of this controller is motivated by our previous paper where we proved that for a queuing model of the uppeak dispatching problem a threshold policy is optimal (in the sense of minimizing the average passenger waiting time) with threshold parameters that depend on the passenger arrival rate. The controller, which we call the concurrent estimation dispatching algorithm ({CEDA}), uses concurrent estimation techniques for discrete-event systems. The {CEDA} allows us to observe the elevator system while it operates under some arbitrary thresholds, and concurrently estimate, in an unobtrusive way, what the waiting time would have been had the system operated under a set of different thresholds. These concurrently estimated waiting times are used to adapt the operating thresholds to match the elevator service rate to a changing passenger arrival rate. Implementation issues relating to the limited state information provided by actual elevator systems are resolved in a way that maintains modest computational requirements and avoids the need for supplemental sensors beyond those already typically provided. Numerical performance results show the advantages of the {CEDA} over currently used dispatching algorithms for uppeak.},
    author = {Pepyne, D. L. and Cassandras, C. G.},
    citeulike-article-id = {4562982},
