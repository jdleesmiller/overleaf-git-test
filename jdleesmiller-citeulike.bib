    journal = {IEEE Transactions on Automation Science and Engineering},
    month = jul,
    number = {3},
    pages = {607--616},
    posted-at = {2010-11-18 14:07:36},
    priority = {2},
    title = {A Collaborative Multiagent {Taxi-Dispatch} System},
