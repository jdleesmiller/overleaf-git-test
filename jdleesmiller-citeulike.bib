    posted-at = {2009-09-09 11:31:20},
    priority = {2},
    title = {A {Neural-Tabu} Search Heuristic for the Real Time Vehicle Routing Problem},
    url = {http://dx.doi.org/10.1007/s10852-008-9082-0},
    volume = {7},
    year = {2008}
}

