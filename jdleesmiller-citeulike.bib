    pages = {135--147},
    posted-at = {2009-05-20 17:43:28},
    priority = {2},
    title = {Idealised models for public transport systems},
    url = {http://dx.doi.org/10.1016/j.ijtm.2005.05.001},
    volume = {2},
    year = {2004}
}

