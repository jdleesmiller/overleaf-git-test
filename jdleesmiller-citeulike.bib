    citeulike-linkout-0 = {http://dx.doi.org/10.1016/j.neuroscience.2004.03.043},
    doi = {10.1016/j.neuroscience.2004.03.043},
    journal = {Neuroscience},
    keywords = {499, file-import-09-05-20},
    number = {3},
