    posted-at = {2009-05-20 17:43:45},
    priority = {2},
    title = {Group Elevator Scheduling with Advanced Traffic Information for Normal Operations and Coordinated Emergency Evacuation},
    url = {http://ieeexplore.ieee.org/xpls/abs_all.jsp?arnumber=1570314},
    year = {2005}
