    priority = {2},
    publisher = {Institute of Transportation Engineers},
    title = {Queue Analysis at Toll and Parking Exit Plazas: A Comparison between Multi-server Queuing Models and Traffic Simulation},
    url = {http://ntlsearch.bts.gov/tris/record/tris/00981493.html},
    year = {2004}
