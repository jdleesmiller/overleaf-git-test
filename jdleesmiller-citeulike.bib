    journal = {International Journal of Adaptive Control and Signal Processing},
    number = {2},
    pages = {103--123},
    posted-at = {2009-09-03 18:02:43},
    priority = {2},
    title = {Hybrid adaptive predictive control for a dynamic pickup and delivery problem including traffic congestion},
    url = {http://dx.doi.org/10.1002/acs.1005},
    volume = {22},
    year = {2008}
}

