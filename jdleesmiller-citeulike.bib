    issn = {1097-0037},
    journal = {Networks},
    number = {3},
    pages = {263--282},
    posted-at = {2010-03-21 18:30:08},
    priority = {2},
    title = {Special cases of traveling salesman and repairman problems with time windows},
    url = {http://dx.doi.org/10.1002/net.3230220305},
    volume = {22},
    year = {1992}
}

