@article{Ghiani2007New,
    abstract = {The <i>dynamic traveling salesman problem</i> is a variant of the classical <i>traveling salesman problem</i> in which service requests arrive in a random fashion. In this paper we propose three new routing policies minimizing the expected total waiting time of all the demands in the system. In particular, we show that two policies are asymptotically optimal in heavy traffic, whereas the third one has a constant factor guarantee of two. We also perform extensive simulations in order to compare the three policies under various arrival rates.},
    author = {Ghiani, Gianpaolo and Quaranta, Antonella and Triki, Chefi},
